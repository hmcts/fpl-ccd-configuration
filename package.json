{
  "name": "fpl-ccd-configuration",
  "version": "1.0.0",
  "description": "FPL CCD e2e tests",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "codeceptjs": "3.0.4",
    "dateformat": "^4.4.1",
    "eslint": "7.17.0",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "glob": "^7.1.6",
    "husky": "^4.3.8",
    "lodash": "^4.17.20",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi": "^1.1.3",
    "mochawesome": "^6.2.1",
    "node-fetch": "^2.6.1",
    "puppeteer": "5.5.0",
    "moment": "2.29.1"
  },
  "scripts": {
    "lint": "eslint e2e",
    "typings": "codeceptjs def && mv steps.d.ts typings/codeceptjs.d.ts",
    "test": "codeceptjs run-multiple parallel",
    "test:functional": "MOCHAWESOME_REPORTFILENAME=functional codeceptjs run-multiple parallel --grep \"${TESTS_SELECTOR:=(?=.*)^(?!.*@smoke-tests)}\" --reporter mocha-multi",
    "test:smoke": "MOCHAWESOME_REPORTFILENAME=smoke codeceptjs run --grep @smoke-tests --reporter mocha-multi"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "license": "MIT"
}
